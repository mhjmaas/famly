{
  "name": "@famly/shared",
  "version": "1.0.0",
  "description": "Shared code and configuration for Famly applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./features": {
      "types": "./dist/features.d.ts",
      "default": "./dist/features.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "famly",
    "shared",
    "features"
  ],
  "private": true,
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
